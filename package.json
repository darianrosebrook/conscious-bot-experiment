{
  "name": "conscious-bot",
  "version": "0.1.0",
  "description": "A conscious AI agent with hierarchical reasoning capabilities",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node scripts/start.js",
    "start:quiet": "node scripts/start.js --quiet",
    "start:progress": "node scripts/start.js --progress",
    "start:debug": "node scripts/start.js --debug",
    "start:production": "node scripts/start.js --production",
    "dev": "node scripts/start.js",
    "build": "turbo build",
    "build:ts": "tsc -b -v tsconfig.build.json",
    "dev:legacy": "pnpm run dev:services",
    "dev:all": "node scripts/dev.js",
    "dev:services": "concurrently \"pnpm run dev:dashboard\" \"pnpm run dev:minecraft\" \"pnpm run dev:cognition\" \"pnpm run dev:memory\" \"pnpm run dev:world\" \"pnpm run dev:planning\" \"pnpm --filter @conscious-bot/core dev\" \"pnpm --filter @conscious-bot/mcp-server dev\"",
    "dev:dashboard": "pnpm --filter @conscious-bot/dashboard dev",
    "dev:minecraft": "pnpm --filter @conscious-bot/minecraft-interface run dev:server",
    "dev:cognition": "pnpm --filter @conscious-bot/cognition run dev:server",
    "dev:memory": "pnpm --filter @conscious-bot/memory run dev:server",
    "dev:world": "pnpm --filter @conscious-bot/world run dev:server",
    "dev:planning": "pnpm --filter @conscious-bot/planning run dev:server",
    "kill": "node scripts/kill-servers.js",
    "status": "node scripts/status.js",
    "health": "node scripts/health-check-diagnostic.js",
    "clear-cognitive-state": "node scripts/clear-cognitive-state.js",
    "clear-cognitive-state:db": "node scripts/clear-cognitive-state.js --db",
    "lint": "turbo lint",
    "docs:lint": "node scripts/docs-boundary-lint.js",
    "test": "turbo test",
    "clean": "turbo clean",
    "type-check": "turbo type-check",
    "agent:dev": "pnpm --filter @conscious-bot/agent dev",
    "core:dev": "pnpm --filter @conscious-bot/core dev",
    "audit": "node docs/audit/audit-runner.ts",
    "audit:architecture": "node docs/audit/audit-runner.ts architecture",
    "audit:integration": "node docs/audit/audit-runner.ts integration",
    "audit:complete": "node docs/audit/audit-runner.ts complete",
    "audit:quality-gates": "node docs/audit/verification-system.ts",
    "audit:integration-map": "node docs/audit/integration-mapper.ts",
    "audit:comprehensive": "node docs/audit/reporting-system.ts",
    "audit:working-specs": "node docs/audit/audit-runner.ts --validate-specs",
    "e2e": "bash scripts/run-e2e.sh",
    "e2e:teardown": "bash scripts/run-e2e.sh --teardown",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:ps": "docker compose ps",
    "docker:logs": "docker compose logs -f",
    "benchmark:mlx": "node scripts/benchmark-mlx.js",
    "moc": "python3 scripts/utils/generate_core_inventory.py",
    "moc:dry-run": "python3 scripts/utils/generate_core_inventory.py --dry-run --no-ai",
    "test:p21": "pnpm --filter @conscious-bot/testkits test:p21 && pnpm --filter @conscious-bot/minecraft-interface test:p21"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@types/node": "^20.19.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cli-progress": "^3.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "listr2": "^10.1.0",
    "ora": "^9.1.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "turbo": "^2.8.2",
    "typescript": "^5.9.2",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.15.0",
  "keywords": [
    "ai",
    "consciousness",
    "cognitive-architecture",
    "minecraft",
    "embodied-ai",
    "reasoning",
    "sterling"
  ],
  "author": "@darianrosebrook",
  "license": "MIT",
  "dependencies": {
    "canvas": "^3.2.0",
    "pnpm": "10.15.0"
  },
  "pnpm": {
    "overrides": {
      "axios": "^1.12.0",
      "esbuild": "^0.25.0",
      "vite": "^5.4.20"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "canvas",
      "cpu-features",
      "esbuild",
      "node",
      "protobufjs",
      "ssh2"
    ],
    "patchedDependencies": {
      "prismarine-viewer@1.33.0": "patches/prismarine-viewer@1.33.0.patch"
    }
  }
}
