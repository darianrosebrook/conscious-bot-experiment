  "devDependencies": {
    "@types/js-yaml": "^4.0.8",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.2",
    "ts-jest": "^29.1.1"
  },
  "keywords": [
    "minecraft",
    "planning",
    "cognitive-architecture",
    "mineflayer"
