{
  "name": "@conscious-bot/minecraft-interface",
  "version": "0.1.0",
  "description": "Minecraft integration layer for conscious bot planning system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mc-smoke": "dist/bin/mc-smoke.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
