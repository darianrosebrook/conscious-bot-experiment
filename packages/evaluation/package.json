{
  "name": "@conscious-bot/evaluation",
  "version": "0.1.0",
  "description": "Evaluation framework for complex multi-step reasoning scenarios",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "benchmarks": "node --loader ts-node/esm src/benchmarking/run-benchmarks.ts",
    "benchmarks:memory": "node --loader ts-node/esm src/benchmarking/run-benchmarks.ts --memory-only",
    "benchmarks:safety": "node --loader ts-node/esm src/benchmarking/run-benchmarks.ts --safety-only",
    "benchmarks:quick": "node --loader ts-node/esm src/benchmarking/run-benchmarks.ts --quick"
  },
  "dependencies": {
    "@conscious-bot/cognition": "workspace:^",
    "@conscious-bot/core": "workspace:^",
    "@conscious-bot/memory": "workspace:^",
    "@conscious-bot/planning": "workspace:^",
    "@conscious-bot/safety": "workspace:^",
    "@conscious-bot/world": "workspace:^",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.9.0",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.1",
    "testcontainers": "^10.9.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "evaluation",
    "testing",
    "cognitive-architecture",
    "reasoning",
    "scenarios"
  ],
  "author": "@darianrosebrook",
  "license": "MIT"
}
